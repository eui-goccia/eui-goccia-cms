{
	"name": "eui-goccia",
	"version": "1.0.0",
	"description": "EUI Goccia",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "pnpm lint && cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
		"dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
		"devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"lint": "biome check .",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start",
		"db:up": "docker compose -f docker-compose.db.yml up -d",
		"db:down": "docker compose -f docker-compose.db.yml down",
		"db:purge": "docker compose -f docker-compose.db.yml down -v && rm -rf payload.sqlite",
		"prepare": "husky"
	},
	"dependencies": {
		"@libsql/client": "^0.15.9",
		"@payloadcms/db-sqlite": "latest",
		"@payloadcms/live-preview-react": "latest",
		"@payloadcms/next": "latest",
		"@payloadcms/plugin-seo": "latest",
		"@payloadcms/richtext-lexical": "latest",
		"@payloadcms/storage-s3": "latest",
		"@payloadcms/translations": "latest",
		"@payloadcms/ui": "latest",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/postcss": "^4.1.11",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.6.1",
		"framer-motion": "^12.23.0",
		"graphql": "^16.11.0",
		"lenis": "^1.3.4",
		"libsql": "^0.5.13",
		"lucide-react": "^0.524.0",
		"mapbox-gl": "^3.13.0",
		"next": "^15.3.5",
		"next-intl": "^4.3.4",
		"next-plausible": "^3.12.4",
		"payload": "latest",
		"plaiceholder": "^3.0.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"react": "^19.1.0",
		"sharp": "^0.34.2",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@types/node": "^22.16.0",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"husky": "^9.1.7",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=20.9.0",
		"pnpm": "^10.12.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"bufferutil",
			"esbuild",
			"sharp",
			"utf-8-validate"
		]
	}
}
